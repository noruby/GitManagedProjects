@article{Brownian_DLS,
author ="van Veluwen and Aernout and Lekkerkerker and Hendrik N. W. and de Kruif and Cornelus G. and Vrij, Agienus",
title  ="Brownian diffusivities of interacting colloidal particles measured by dynamic light scattering",
journal  ="Faraday Discuss. Chem. Soc.",
year  ="1987",
volume  ="83",
issue  ="0",
pages  ="59-67",
publisher ="The Royal Society of Chemistry",
doi  ="10.1039/DC9878300059",
url  ="http://dx.doi.org/10.1039/DC9878300059",
abstract  ="In a system of interacting colloidal particles one can distinguish collective diffusion{,} short-time and long-time self-diffusion and exchange diffusion. The meanings of and connections between these diffusivities are established. It is shown how these various Brownian diffusivities may be obtained from dynamic light scattering measurements on monodisperse and polydisperse systems. Experimental results for both dilute and concentrated sterically stabilized silica dispersions are presented. The experimental concentration dependence of the various diffusivities is compared with theoretical predictions."}

