speed_of_light <- 299792458 #m/s
permittivity_vacuum <- 8.854187817*10^(-12)
num<-50
scattering_rate <- seq(220,380, length=50) #cm^-1 (<380 cm^-1 for grain size of >56nm)
scattering_time <- 1/ 2/ pi / speed_of_light/ scattering_rate 
plasma_frequency <-70000 #cm^-1
dielectric_background <- 1
operating_frequency <- 943 #cm^-1
epsilon <- dielectric_background - plasma_frequency^2 / ( operating_frequency^2 + 1i*operating_frequency*scattering_rate )
wavenumber_x <- operating_frequency* sqrt(epsilon/(1+epsilon)) #1/cm
propagation_length <- 1/(2*Im( wavenumber_x ))
wavenumber_z_air <- operating_frequency* sqrt(1/(1+epsilon)) #1/cm
penetration_length_air <- -1/(2*Im( wavenumber_z_air ))
wavenumber_z_metal <- operating_frequency* sqrt(epsilon^2/(1+epsilon)) #1/cm
penetration_length_metal <- 1/(2*Im( wavenumber_z_metal ))
conductivity <- plasma_frequency^2*permittivity_vacuum/(scattering_rate-1i*operating_frequency)
